{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/userOb",
  "type": "object",
  "properties": {
    "deviceId": {
      "id": "/deviceId",
      "type": "string"
    },
    "sessionId": {
      "id": "/sessionId",
      "type": "string"
    },
    "location": {
      "id": "/location",
      "type": "object",
      "properties": {
        "latitude": {
          "id": "/location/latitude",
          "type": "number"
        },
        "longitude": {
          "id": "/location/longitude",
          "type": "number"
        }
      }
    },
    "ob": {
      "id": "/ob",
      "type": "string"
    }
  },
  "required": [
    "deviceId",
    "sessionId",
    "location",
    "ob"
  ]
}